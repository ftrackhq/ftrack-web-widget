  "homepage": "https://ftrack.com",
  "devDependencies": {
    "eslint": "^8.50.0",
    "eslint-config-react-app": "^7.0.1",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vite-plugin-dts": "^3.6.0"
  }
